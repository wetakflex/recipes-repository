{
    "manifests": {
        "taklearning/alejandria-bundle": {
            "manifest": {
                "bundles": {
                    "AlejandriaBundle\\AlejandriaBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%"
                },
                "env": {
                    "paypal_express_checkout_username": "",
                    "paypal_express_checkout_password": "&",
                    "paypal_express_checkout_signature": "",
                    "paypal_express_checkout_sandbox": true,
                    "redsys_merchant_code": "",
                    "redsys_terminal": "",
                    "redsys_secret_key": "",
                    "redsys_sandbox": true
                },
                "aliases": ["wetak_alejandria"]
            },
            "files": {
                "config/packages/taklearning_imports.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: '@AlejandriaBundle/Resources/config/config.yml' }",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/taklearning_alejandria.yaml": {
                    "contents": [
                        "alejandria:",
                        "    campus:",
                        "        active: false",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/taklearning_payum.yaml": {
                    "contents": [
                        "payum:",
                        "    gateways:",
                        "        paypal_express_checkout:",
                        "            factory: paypal_express_checkout",
                        "            username: '%env(paypal_express_checkout_username)%'",
                        "            password: '%env(paypal_express_checkout_password)%'",
                        "            signature: '%env(paypal_express_checkout_signature)%'",
                        "            sandbox: '%env(paypal_express_checkout_sandbox)%'",
                        "        redsys:",
                        "            factory: redsys",
                        "            merchant_code: '%env(redsys_merchant_code)%'",
                        "            terminal: '%env(redsys_terminal)%'",
                        "            secret_key: '%env(redsys_secret_key)%'",
                        "            sandbox: '%env(redsys_sandbox)%'",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/taklearning_nelmio_cors.yaml": {
                    "contents": [
                        "nelmio_cors:",
                        "    paths:",
                        "        '^/':",
                        "            allow_origin: ['*']",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "87ae1a0850a7529361dff693035c105012aa200d"
        }
    }
